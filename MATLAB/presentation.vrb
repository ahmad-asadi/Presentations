\frametitle{Reading values of a particle matrix}
\begin{block}{}
\begin{itemize}
\item get value of all cells on row 3 of matrix $A$
\java
\begin{lstlisting}
>> A(3,:)
\end{lstlisting}
\item get value of all cells on col 2 of matrix $A$
\java
\begin{lstlisting}
>> A(:,2)
\end{lstlisting}
\item get value of all cells of matrix $A$
\java
\begin{lstlisting}
>> A(:,:)
\end{lstlisting}
\end{itemize}
\end{block}
